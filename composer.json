{
  "name": "workerman/webman",
  "type": "project",
  "keywords": [
    "high performance",
    "http service"
  ],
  "homepage": "https://www.workerman.net",
  "license": "MIT",
  "description": "High performance HTTP Service Framework.",
  "authors": [
    {
      "name": "walkor",
      "email": "walkor@workerman.net",
      "homepage": "https://www.workerman.net",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "walkor@workerman.net",
    "issues": "https://github.com/walkor/webman/issues",
    "forum": "https://wenda.workerman.net/",
    "wiki": "https://workerman.net/doc/webman",
    "source": "https://github.com/walkor/webman"
  },
  "require": {
    "php": ">=7.4",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-simplexml": "*",
    "ext-curl": "*",
    "ext-bcmath": "*",
    "php-curl-class/php-curl-class": "^9.5",
    "workerman/webman-framework": "^1.5.0",
    "monolog/monolog": "^2.0",
    "webman/console": "^1.3",
    "webman/think-orm": "^1.1",
    "webman/redis-queue": "^1.2",
    "webman/captcha": "^1.0",
    "webman/think-cache": "^1.0",
    "webman/event": "^1.0",
    "symfony/var-dumper": "^5.4",
    "illuminate/database": "^8.83",
    "illuminate/pagination": "^8.83",
    "illuminate/events": "^8.83",
    "symfony/yaml": "^5.4",
    "topthink/think-validate": "^2.0",
    "illuminate/redis": "^8.83",
    "symfony/cache": "^5.4",
    "topthink/think-template": "^2.0",
    "mathieuviossat/arraytotexttable": "^1.0",
    "phpoffice/phpspreadsheet": "^1.29",
    "next/var-dumper": "^0.1.0",
    "overtrue/wechat": "^5.30",
    "robmorgan/phinx": "^0.14.0",
    "alibabacloud/dysmsapi-20170525": "^2.0",
    "shopwwi/webman-filesystem": "^1.1",
    "shopwwi/filesystem-oss": "^1.0",
    "intervention/image": "^2.7",
    "hg/apidoc": "^5.2",
    "endroid/qr-code": "^4.8",
    "chance-fyi/operation-log": "^2.0",
    "shiroi/xhprof-webman": "^0.0.5",
    "workerman/crontab": "^1.0"
  },
  "suggest": {
    "ext-event": "For better performance. "
  },
  "autoload": {
    "psr-0" : {
      "": "extend/"
    },
    "psr-4": {
      "": "./",
      "app\\": "./app",
      "App\\": "./app",
      "app\\View\\Components\\": "app/admin/view/components"
    },
    "files": [
      "./support/helpers.php",
      "./app/common.php"
    ]
  },
  "scripts": {
    "post-package-install": [
      "support\\Plugin::install"
    ],
    "post-package-update": [
      "support\\Plugin::install"
    ],
    "pre-package-uninstall": [
      "support\\Plugin::uninstall"
    ]
  },
  "config": {
    "allow-plugins": {
      "easywechat-composer/easywechat-composer": true
    }
  }
}
